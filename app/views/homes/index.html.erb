<ol>
  <% @page.links.each do |link| %>
    <% if link.href != nil && link.text != nil%>
      <% url = link.href.gsub(/(.*)aol(.*)/,"") %>
      <% url = url.gsub(/(.*)google.com(.*)/,"") %>
      <% url = url.gsub(/(.*)search?(.*)/,"") %>
      <% url = url.gsub(/(.*)about?(.*)/,"") %>
      <% url = url.gsub(/javascript(.*)/,"") %>
      <% text = link.text.gsub(/(.*)More Â»(.*)/,"") %>
      <% text = text.gsub(/(.*)Mobile Site(.*)/,"") %>
      <% if url.length != 0 && text.length != 0 %>
        <li><%= link_to text, url %></li>
      <% end %>
    <% end %>
  <% end %>
</ol>
