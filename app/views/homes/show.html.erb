@bing = Mechanize.new
@page = @bing.get('https://search.yahoo.com/')
@bing_form = @page.form()
@bing_form.q = 'ruby mechanize'
@page = @bing.submit(@bing_form)

<ul>
  <% @page.links.each do |link| %>
    <% if link.href != nil && link.text != nil%>
      <% url = link.href.gsub(/^(?!http(.*)$).*/,"") %>
      <% url = url.gsub(/(.*)microsoft.com(.*)/,"") %>
      <% if url.length != 0 && link.text != 0 %>
        <li><%= link_to link.text, url %></li>
      <% end %>
    <% end %>
  <% end %>
</ul>

